{
    "version": "0.1.0",
    "command": "sh",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [{
            "taskName": "build",
            "args": ["-c", "clang-omp++ -fopenmp -std=c++14 -Wall -Wno-missing-braces -Wno-unused-variable -Wno-unused-parameter -Wextra -pthread -lgsl -lgslcblas -lm -o ./bin/run src/torus.cpp src/fukushima/elliptic_integral.cpp && ./bin/run"],
            "isBuildCommand": true
        },
        {
            "taskName": "run",
            "args": ["./bin/run"],
            "isTestCommand": true
        }
    ]
}