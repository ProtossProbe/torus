{
    "version": "0.1.0",
    "command": "sh",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [{
            "taskName": "build",
            "args": ["-c", "clang-omp++ -fopenmp -std=c++14 -Wall -Wno-missing-braces -Wno-unused-variable -Wno-unused-parameter -Wextra -pthread -o run src/torus_run.cpp src/gauss_legendre.c && ./run"],
            "isBuildCommand": true
        },
        {
            "taskName": "run",
            "args": ["./run"],
            "isTestCommand": true
        }
    ]
}